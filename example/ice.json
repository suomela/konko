{
    "source": {
        "ice-india-spoken": [ "corpus/ice/ICE India/Corpus/S*.txt" ],
        "ice-india-written": [ "corpus/ice/ICE India/Corpus/W*.txt" ]
    },
    "skip-files": [
        "corpus/ice/ICE India/Corpus/S1a-015.txt"
    ],
    "output-dir": "output",
    "encoding": "1252",
    "context": 100,
    "search": {
        "ness": ".*(n[e']ss.*|snss)",
        "ity": ".*([ie]t(y|ie).*|vty)"
    },
    "word": "\\w+([-']\\w+)*",
    "tag": "<[^<> ]+>",
    "delete": [
        ["<.*#.*>"],
        ["<O>", "</O>"],
        ["<X>", "</X>"],
        ["<&>", "</&>"]
    ],
    "show-prev": "<\\$[A-Z]>",
    "search-ignore-case": true,
    "word-ignore-case": false,
    "tag-ignore-case": false
}
