{
    "source": {
        "ice-canada-spoken": [ "corpus/ice/ICE Canada/Corpus/S*.TXT" ],
        "ice-canada-written": [ "corpus/ice/ICE Canada/Corpus/W*.TXT" ]
    },
    "output-dir": "output/ice-canada",
    "encoding": "1252",
    "context": 100,
    "search": {
        "ness": ".*(n[e']ss.*|snss)",
        "ity": ".*([ie]t(y|ie).*|vty)"
    },
    "word": "\\w+([-']\\w+)*",
    "tag": "<[^<> ]+>",
    "delete": [
        ["<.*#.*>"],
        ["<O>", "</O>"],
        ["<X>", "</X>"],
        ["<&>", "</&>"]
    ],
    "show-prev": "<\\$.*>",
    "search-ignore-case": true,
    "word-ignore-case": false,
    "tag-ignore-case": false
}
